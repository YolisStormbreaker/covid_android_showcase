<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="2400">
       <KeyFrameSet>
           <KeyAttribute
               motion:motionTarget="@+id/logo_imageView"
               motion:framePosition="0"
               android:scaleX="1" />
           <KeyAttribute
               motion:motionTarget="@+id/logo_imageView"
               motion:framePosition="5"
               android:scaleX="0.7" />
           <KeyAttribute
               motion:motionTarget="@+id/logo_imageView"
               motion:framePosition="8"
               android:scaleX="1" />
           <KeyAttribute
               motion:motionTarget="@+id/logo_imageView"
               motion:framePosition="15"
               android:scaleX="0.7" />
           <KeyAttribute
               motion:motionTarget="@+id/logo_imageView"
               motion:framePosition="18"
               android:scaleX="1" />
           <KeyAttribute
               motion:motionTarget="@+id/logo_imageView"
               motion:framePosition="40"
               android:scaleX="1" />
           <KeyAttribute
               motion:motionTarget="@+id/logo_imageView"
               motion:framePosition="45"
               android:scaleX="0.7" />
           <KeyAttribute
               motion:motionTarget="@+id/logo_imageView"
               motion:framePosition="48"
               android:scaleX="1" />
           <KeyAttribute
               motion:motionTarget="@+id/logo_imageView"
               motion:framePosition="55"
               android:scaleX="0.7" />
           <KeyAttribute
               motion:motionTarget="@+id/logo_imageView"
               motion:framePosition="58"
               android:scaleX="1" />
           <KeyAttribute
               motion:motionTarget="@+id/logo_imageView"
               motion:framePosition="70"
               android:scaleX="1" />
           <KeyAttribute
               motion:motionTarget="@+id/logo_imageView"
               motion:framePosition="100"
               android:scaleX="50" />
           <KeyAttribute
               motion:motionTarget="@+id/logo_imageView"
               motion:framePosition="0"
               android:alpha="1" />
           <KeyAttribute
               motion:motionTarget="@+id/logo_imageView"
               motion:framePosition="58"
               android:alpha="1" />
           <KeyAttribute
               motion:motionTarget="@+id/logo_imageView"
               motion:framePosition="80"
               android:alpha="0.1" />
           <KeyAttribute
               motion:motionTarget="@+id/logo_imageView"
               motion:framePosition="100"
               android:alpha="0.1" />
       </KeyFrameSet>
        <KeyFrameSet>

            <KeyAttribute
                motion:motionTarget="@+id/logo_imageView"
                motion:framePosition="0"
                android:scaleY="1" />
            <KeyAttribute
                motion:motionTarget="@+id/logo_imageView"
                motion:framePosition="5"
                android:scaleY="0.7" />
            <KeyAttribute
                motion:motionTarget="@+id/logo_imageView"
                motion:framePosition="8"
                android:scaleY="1" />
            <KeyAttribute
                motion:motionTarget="@+id/logo_imageView"
                motion:framePosition="15"
                android:scaleY="0.7" />
            <KeyAttribute
                motion:motionTarget="@+id/logo_imageView"
                motion:framePosition="18"
                android:scaleY="1" />
            <KeyAttribute
                motion:motionTarget="@+id/logo_imageView"
                motion:framePosition="40"
                android:scaleY="1" />
            <KeyAttribute
                motion:motionTarget="@+id/logo_imageView"
                motion:framePosition="45"
                android:scaleY="0.7" />
            <KeyAttribute
                motion:motionTarget="@+id/logo_imageView"
                motion:framePosition="48"
                android:scaleY="1" />
            <KeyAttribute
                motion:motionTarget="@+id/logo_imageView"
                motion:framePosition="55"
                android:scaleY="0.7" />
            <KeyAttribute
                motion:motionTarget="@+id/logo_imageView"
                motion:framePosition="58"
                android:scaleY="1" />
            <KeyAttribute
                motion:motionTarget="@+id/logo_imageView"
                motion:framePosition="70"
                android:scaleY="1" />
            <KeyAttribute
                motion:motionTarget="@+id/logo_imageView"
                motion:framePosition="100"
                android:scaleY="50" />
        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>